/CustomTable.js",["150"],"import styles from './CustomTable.module.css'\nimport Row from \"./Row\";\nimport TableTitle from \"./TableTitle\";\n\nconst Table = (props) => {\n\n\n    const getRows = (rows, columnHeaders) => {\n        return rows.map(row => {\n            return <Row key={row.id} columns={columnHeaders} data={row}/>\n        })\n    }\n\n    const fillTable = (data, columnHeaders, layer) => {\n        if (Array.isArray(data)) {\n            return (\n                <table key={data.id} className={styles.table}>\n                    <tbody className={styles[\"data-table\"]}>\n                    <Row columns={columnHeaders}/>\n                    {getRows(data, columnHeaders)}\n                    </tbody>\n                </table>)\n        }\n        return Object.keys(data).map((key, index) => {\n            // console.log(\"key: \" + key)\n            // console.log(\"value: \" + JSON.stringify(data[key]))\n            const isArray = Array.isArray(data[key])\n            // console.log(\"isArray: \" + isArray)\n            return (\n                <TableTitle key={index + key} text={key} layer={layer}>\n                    {fillTable(data[key], columnHeaders, layer+1)}\n                </TableTitle>\n            )\n        })\n    }\n\n    return (\n        <div className={styles[\"table-main\"]}>\n            {fillTable(props.data, props.columnHeaders, 0)}\n            {/*<TableTitle text='Table header'>*/}\n            {/*<table className={styles[\"table-fill\"]}>*/}\n            {/*<table className={styles.table}>*/}\n            {/*<thead>*/}\n            {/*<tr className={styles['header-row']}>*/}\n            {/*    {getHeaders()}*/}\n            {/*</tr>*/}\n            {/*</thead>*/}\n            {/*<tbody className={styles[\"table-hover\"]}>*/}\n            {/*<tbody className={styles[\"data-table\"]}>*/}\n            {/*<Row columns={visibleColumns}/>*/}\n            {/*{getRows()}*/}\n            {/*</tbody>*/}\n            {/*</table>*/}\n            {/*</TableTitle>*/}\n        </div>\n    )\n}\n\nexport default Table","/auto/dev_users/sdolgov/IdeaProjects/Study/Budget/budget-front/src/components/UI/Table/TableTitle.js",[],"/auto/dev_users/sdolgov/IdeaProjects/Study/Budget/budget-front/src/components/UI/Table/Row.js",["151"],"import styles from './Row.module.css'\nimport Cell from \"./Cell\";\n\nconst DUMMY = {\n    month: 'May',\n    sales: '$ 98,000.00'\n}\n\nconst Row = (props) => {\n    // console.log('columns: ' + props.columns)\n    const getCells = () => {\n        return Object.keys(props.columns).map((column, index) => {\n            // console.log(\"props.data[column]: \" + props.data[column])\n            const value = props.data ? props.data[column] : props.columns[column]\n            return <Cell key={index} value={value}/>\n        })\n\n        // return Object.keys(props.data).map(key => {\n        //     return <Cell value={props.data[key]}/>\n        // })\n    }\n    // const getCells = () => {\n    //     return Object.keys(DUMMY).map(key => {\n    //         return <Cell value={DUMMY[key]}/>\n    //     })\n    // }\n\n    const rowClass = props.data ? `${styles['data-row']}` : `${styles['header-row']}`\n    const classes = `${styles.row} ${rowClass}`\n\n    return (\n        <tr className={classes}>\n            {getCells()}\n        </tr>\n    )\n}\n\nexport default Row","/auto/dev_users/sdolgov/IdeaProjects/Study/Budget/budget-front/src/components/UI/Table/ColumnHeader.js",[],"/auto/dev_users/sdolgov/IdeaProjects/Study/Budget/budget-front/src/components/UI/Table/TitlePanel.js",[],"/auto/dev_users/sdolgov/IdeaProjects/Study/Budget/budget-front/src/components/UI/Table/Cell.js",[],"/auto/dev_users/sdolgov/IdeaProjects/Study/Budget/budget-front/src/components/UI/Button/ArrowButton.js",[],"/auto/dev_users/sdolgov/IdeaProjects/Study/Budget/budget-front/src/components/UI/Table/Sheet.js",["152","153"],{"ruleId":"154","replacedBy":"155"},{"ruleId":"156","replacedBy":"157"},{"ru